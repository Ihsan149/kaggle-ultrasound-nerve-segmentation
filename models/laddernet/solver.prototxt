train_net: "models/laddernet/train.prototxt"  		
test_net: "models/laddernet/val.prototxt"  
test_initialization: true
test_iter: 16
test_interval: 100
base_lr: 0.01
iter_size: 1
lr_policy: "step"
gamma: 0.1
stepsize: 4000
display: 20
momentum: 0.9
max_iter: 12000
weight_decay: 0.0005
snapshot: 500
snapshot_prefix: "data/models/segnet"  	
solver_mode: GPU